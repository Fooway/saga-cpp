<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="antlr"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(ANTLR_ROOT)\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="antlr.lib"
		AdditionalLibraryDirectories="&quot;$(ANTLR_ROOT)\lib\$(ConfigurationName)&quot;"
	/>
	<UserMacro
		Name="ANTLR_ROOT"
		Value="$(SAGA_ANTLR_ROOT)"
	/>
</VisualStudioPropertySheet>
