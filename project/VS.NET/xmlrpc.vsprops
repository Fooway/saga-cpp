<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="xmlrpc"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(SAGA_XMLRPC_ROOT)\src"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="$(XMLRPC_LIB) Ws2_32.lib libeay32.lib ssleay32.lib"
		AdditionalLibraryDirectories="$(SAGA_XMLRPC_ROOT)\$(ConfigurationName)"
	/>
	<UserMacro
		Name="XMLRPC_LIB"
		Value="xmlrpc.lib"
	/>
</VisualStudioPropertySheet>
