<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="openssl"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(SAGA_OPENSSL_ROOT)\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="libeay32MD.lib ssleay32MD.lib "
		AdditionalLibraryDirectories="$(SAGA_OPENSSL_ROOT)\lib\VC"
	/>
	<UserMacro
		Name="OPENSSL_ROOT"
		Value="$(SAGA_OPENSSL_ROOT)"
	/>
</VisualStudioPropertySheet>
