<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="readline"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(SAGA_READLINE_ROOT)\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalDependencies="history.lib readline.lib"
		AdditionalLibraryDirectories="$(SAGA_READLINE_ROOT)\lib"
	/>
	<UserMacro
		Name="READLINE_ROOT"
		Value="$(SAGA_READLINE_ROOT)"
	/>
</VisualStudioPropertySheet>
