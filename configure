  depfiles=$SAGA_DEP_FILES
  depfiles_nv=

  # Extract the first word of "dpkg", so it can be a program name with args.
set dummy dpkg; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_SAGA_DPKG+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $SAGA_DPKG in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_SAGA_DPKG="$SAGA_DPKG" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_SAGA_DPKG="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
SAGA_DPKG=$ac_cv_path_SAGA_DPKG
if test -n "$SAGA_DPKG"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $SAGA_DPKG" >&5
$as_echo "$SAGA_DPKG" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi



  if test "$SAGA_DPKG" = ""; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: DPKG not found - not a debian system?" >&5
$as_echo "DPKG not found - not a debian system?" >&6; }
  else

    SAGA_DEB_DEPENDENCIES=""
    first=yes
    oldpackage="NONE"

    for  depfile in $depfiles;
    do

      depfile=`echo $depfile | sed -e 's|//|/|g'`
      package=`$SAGA_DPKG -S $depfile 2>>/dev/null | cut -sf 1 -d ':'`

      if test "" = "$package"; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: Cannot find debian package for $depfile" >&5
$as_echo "Cannot find debian package for $depfile" >&6; }
        continue
      fi

      # AC_MSG_NOTICE([$depfile: $package])

      if test "$oldpackage" = "$package"; then
        # avoid repetetetive entries, to some extend
        continue;
      fi

      version=`$SAGA_DPKG -p $package 2>>/dev/null | grep -e '^Version:' | cut -sf 2 -d ' '`

      if test "" = "$version"; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: Cannot find debian package version for $depfile" >&5
$as_echo "Cannot find debian package version for $depfile" >&6; }
        continue
      fi

      # this is what, for example, ubuntu 11.x reposrt for the package
      # openssh-client: 1:5.8p1-1ubuntu3
      # Google tells me that this contains version 5.8.x of the ssh tools,
      # so we strip whatever is before the colon, and hope that scheme is
      # universally valid.  haha.
      version=`echo $version | sed -e "s/^.*\://g"`

      vmajor=`echo $version | cut -sf 1 -d '.'`
      vminor=`echo $version | cut -sf 2 -d '.'`
      vmin="$vmajor.$vminor"

      # boost version can only differ by sub-minor version, other packages can
      # differ by minor versions...
      boostmatch=`expr "$package" ":" ".*boost.*"`

      if test "0" = "$boostmatch"; then
        vmax="`expr 1 + $vmajor`.0"
      else
        vmax="$vmajor.`expr 1 + $vminor`"
      fi

      if ! test "$first" == "yes"; then
        SAGA_DEB_DEPENDENCIES="$SAGA_DEB_DEPENDENCIES, "
      fi

      SAGA_DEB_DEPENDENCIES="$SAGA_DEB_DEPENDENCIES$package (>= $vmin)"

      if ! test "." = `expr substr $vmax 1 1`; then
        SAGA_DEB_DEPENDENCIES="$SAGA_DEB_DEPENDENCIES, $package (<< $vmax)"
      fi

      first=no
      oldpackage=$package
    done

    for  depfile in $depfiles_nv;
    do

      depfile=`echo $depfile | sed -e 's|//|/|g'`
      package=`$SAGA_DPKG -S $depfile 2>>/dev/null | cut -sf 1 -d ':'`

      if test "" = "$package"; then
        { $as_echo "$as_me:${as_lineno-$LINENO}: result: Cannot find debian package for $depfile" >&5
$as_echo "Cannot find debian package for $depfile" >&6; }
        continue
      fi

      # AC_MSG_NOTICE([$depfile: $package])

      if test "$oldpackage" = "$package"; then
        # avoid repetetetive entries, to some extend
        continue;
      fi

      if ! test "$first" == "yes"; then
        SAGA_DEB_DEPENDENCIES="$SAGA_DEB_DEPENDENCIES, "
      fi

      SAGA_DEB_DEPENDENCIES="$SAGA_DEB_DEPENDENCIES$package"

      first=no
      oldpackage=$package
    done

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $SAGA_DEB_DEPENDENCIES" >&5
$as_echo "$SAGA_DEB_DEPENDENCIES" >&6; }


    { $as_echo "$as_me:${as_lineno-$LINENO}: checking debian architecture" >&5
$as_echo_n "checking debian architecture... " >&6; }
    deb_arch=`$SAGA_DPKG --print-architecture`
    if test "$deb_arch" == ""; then
      SAGA_DEB_ARCH=$SAGA_ARCH
    else
      SAGA_DEB_ARCH=$deb_arch
    fi
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $SAGA_DEB_ARCH" >&5
$as_echo "$SAGA_DEB_ARCH" >&6; }


    { $as_echo "$as_me:${as_lineno-$LINENO}: checking debian package version" >&5
$as_echo_n "checking debian package version... " >&6; }
    SAGA_DEB_VERSION=$SAGA_VERSION
    test -f PACKAGE && SAGA_DEB_VERSION=`grep '^VERSION:' PACKAGE | sed -e 's/^VERSION: *//'`
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $SAGA_DEB_VERSION" >&5
$as_echo "$SAGA_DEB_VERSION" >&6; }

    { $as_echo "$as_me:${as_lineno-$LINENO}: checking debian package name" >&5
$as_echo_n "checking debian package name... " >&6; }
    SAGA_DEB_PACKAGE="NONE"
    test -f PACKAGE && SAGA_DEB_PACKAGE=`grep '^PACKAGE:' PACKAGE | sed -e 's/^PACKAGE: *//'`
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $SAGA_DEB_PACKAGE" >&5
$as_echo "$SAGA_DEB_PACKAGE" >&6; }

    ###########################################################
    #
    # with the thus gathered information, we can actually
    # create a debian control file
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking debian package control file" >&5
$as_echo_n "checking debian package control file... " >&6; }
    SAGA_DEB_CONTROL=$SAGA_DEB_PACKAGE.deb.ctrl

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: creating $SAGA_DEB_CONTROL" >&5
$as_echo "creating $SAGA_DEB_CONTROL" >&6; }

    cat <<EOT >$SAGA_DEB_CONTROL

Package:        $SAGA_DEB_PACKAGE
Version:        $SAGA_DEB_VERSION
Section:        devel
Priority:       optional
Architecture:   $SAGA_DEB_ARCH
Essential:      no
Depends:        $SAGA_DEB_DEPENDENCIES
Pre-Depends:
Recommends:
Suggests:
Installed-Size: ###SIZE###
Maintainer:     Andre Merzky [andre--saga@merzky.net]
Conflicts:
Replaces:
Provides:
Description:    http://saga-project.org/
EOT
    #
    ###########################################################







  fi # !DPKG



  depfiles=$SAGA_DEP_FILES
  depfiles_nv=

  # Extract the first word of "rpm", so it can be a program name with args.
set dummy rpm; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_SAGA_RPM+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $SAGA_RPM in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_SAGA_RPM="$SAGA_RPM" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_SAGA_RPM="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
SAGA_RPM=$ac_cv_path_SAGA_RPM
if test -n "$SAGA_RPM"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $SAGA_RPM" >&5
$as_echo "$SAGA_RPM" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  # Extract the first word of "rpmbuild", so it can be a program name with args.
set dummy rpmbuild; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_SAGA_RPMBUILD+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $SAGA_RPMBUILD in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_SAGA_RPMBUILD="$SAGA_RPMBUILD" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_SAGA_RPMBUILD="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
