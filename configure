    { $as_echo "$as_me:${as_lineno-$LINENO}: checking redhat package name" >&5
$as_echo_n "checking redhat package name... " >&6; }
    SAGA_RPM_PACKAGE="NONE"
    test -f PACKAGE && SAGA_RPM_PACKAGE=`grep '^PACKAGE:' PACKAGE | sed -e 's/^PACKAGE: *//'`
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: $SAGA_RPM_PACKAGE" >&5
